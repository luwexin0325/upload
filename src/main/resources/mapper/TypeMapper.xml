<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lwx.utils.dao.ITypeDao">
    <sql id="Base_Column_list">id,type AS type,car_no AS carNo</sql>
    <insert id="add">
        INSERT INTO type(id,type,car_no)VALUES(#{id,jdbcType=VARCHAR},#{type,jdbcType=VARCHAR},#{carNo,jdbcType=VARCHAR})
    </insert>
</mapper>